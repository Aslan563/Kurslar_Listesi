@model List<Tag>
<div class="card">
    <div class="card title">
      <h5>Tags</h5>
    </div>
  
    <div class="p-1">
      <ul class="list-inline d-flex flex-wrap gap-2">
        @foreach(var item in Model){
         <li class="list-inline-item my-2"><a href="/posts/tag/@item.Url" class="btn-sm btn-outline-secondary ">@item.Text</a></li>
        }
      </ul>
    </div>
    
   </div>